import java.util.*;
public class search{
    
    static int search(int arr[],int x){
        
    int n =arr.length;
    for(int i=0;i<n;i++){
        if(arr[i]==x){
            return 1;}
    }

      return -1;  
    }

    public static void main(String args[]){
        
    int[] arr = {1,2,3,4,5};
    int x = 4;
    
    int res = search(arr,x);
    if(res==-1){
    System.out.print("not Found ");}
    
    else{
        System.out.print(" Found");
    }

    }
}
